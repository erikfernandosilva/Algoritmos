package com.mycompany.mavenproject8;

/**
 *
 * @author ferna
 */

/*
Este programa mostra os números entre 1 e 500
que são múltiplos de 3 e 5 simultaneamente.
*/

public class Main {
    public static void main(String[] args) {
        int cont = 1;
        while (cont <= 500){
            if (((cont % 3) == 0) && ((cont % 5) == 0))
                System.out.println(cont);
            cont++;
        }
    }
}
