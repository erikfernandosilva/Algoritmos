package com.mycompany.mavenproject9;

/**
 *
 * @author ferna
 */

/*
Este programa realiza a soma dos n√∫meros entre 1 e 100
*/

public class Main {
    public static void main(String[] args) {
        int cont,result;
        cont = 1;
        result = 0;
        while (cont <= 100){
            result = result + cont;
            System.out.println(result);
            cont++;
        }
    }
}
